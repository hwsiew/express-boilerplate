{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "description": "Spin off your application development right away with the necessary function you need.",
  "main": "index.js",
  "scripts": {
    "init": "node ./scripts/init.js",
    "start": "node ./server/index.js",
    "start:dev": "nodemon -L ./server/index.js",
    "docker:up": "docker-compose -f docker-compose.yaml -f docker-compose.prod.yaml up -d",
    "docker:up:dev": "docker-compose -f docker-compose.yaml -f docker-compose.dev.yaml up -d",
    "docker:down": "docker-compose down",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^5.12.13",
    "passport": "^0.4.1"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.7"
  }
}
